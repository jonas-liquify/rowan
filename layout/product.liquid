  function findImg(cab, ctopName, bodyName, cabTop, cabBody){
    let m = model.products.find(e=>e.name==cab)
    let mcodeCtop = model.materialgroups.find(m=>m.name.trim() == ctopName.trim()).materials.find(n=>n.name.trim()==cabTop.trim());
    let mcodeBody = model.materialgroups.find(m=>m.name.trim() == bodyName.trim()).materials.find(n=>n.name.trim()==cabBody.trim());
    let matOrdered = [];
    //created an array of the objects and their data, then sort it to match the imagemap
    let cTopOb = { data: mcodeCtop};
    let bodyOb = { data: mcodeBody};
    matOrdered.push(bodyOb, cTopOb);
     matOrdered.sort(function(a, b) {
        var textA = a.data.multimaterials;
        var textB = b.data.multimaterials;
        return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;
      }
    );
    let msets = [];
    let mmats = [];
    msets.push(m.modelset);
    matOrdered.forEach(function(obj){
      mmats.push(obj.data.multimaterials[0]);
    });
    let img = imagemappingdata.find(i=> JSON.stringify(i.modelsets)==JSON.stringify(msets) && JSON.stringify(i.multimaterials) == JSON.stringify(mmats) );
    if(img != null)
    {
      return img.image;
    }
    else{
      return "";
    }
  }
</script>{% endif %}
