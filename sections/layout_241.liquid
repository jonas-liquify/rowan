<div class="padding-global"><div class="container-large"><div class="padding-section-large"><div class="layout241_component"><div class="margin-bottom margin-xxlarge"><div class="max-width-large">{% if section.settings.checkbox_show_tagline == true %}
<div class="margin-bottom margin-xsmall"><div class="text-style-tagline">
{{ section.settings.text_tagline }}
</div>
</div>{% endif %}
<div class="margin-bottom margin-small"><h2 class="heading-style-h2">
{{ section.settings.text_main_headline }}
</h2>
</div><p class="text-size-medium">
{{ section.settings.text_headline_body }}
</p>
</div></div><div li-content-for-blocks="" class="w-layout-grid layout241_list">
{% for block in section.blocks %}
    {% case block.type %}
        {% when '@app' %}
            {% render block %}
    {% when 'key-feature-block' %}
            <div li-block="Key Feature Block" class="layout241_item">
                    
                    <div class="margin-bottom margin-small"><div class="layout241_item-icon-wrapper"><img loading="lazy" alt="{{ block.settings.image_icon.alt }}" src="{{ block.settings.image_icon | img_url: 'master' }}" class="icon-1x1-medium">
</div></div>
                    
                    <div class="margin-bottom margin-small"><h3 class="heading-style-h4">
{{ block.settings.text_block_headline }}
</h3>
</div>
                    
                    <div> {{ block.settings.richtext_block_text }} </div>

                    
                </div>
        {% endcase %}
{% endfor %}

</div>

{% if section.settings.checkbox_show_cta_buttons == true %}
<div class="margin-top margin-xxlarge"><div class="button-group align-left"><a href="{{ section.settings.url_button_2_url }}" class="button is-link-alternate w-inline-block"> <div>{{ section.settings.text_button_2_text }}</div> </a>
<a href="{{ section.settings.url_button_1_url }}" class="button is-secondary w-inline-block"> <div>{{ section.settings.text_button_1_text }}</div> </a>
</div></div>{% endif %}
</div></div></div></div> {% schema %} {"tag":"section","name":"Layout 241","class":"section_layout241","blocks":[{"name":"Key Feature Block","type":"key-feature-block","settings":[{"id":"image_icon","type":"image_picker","label":"Icon"},{"id":"text_block_headline","type":"text","label":"Block Headline","default":"Key Features of Our 3D Planner"},{"id":"richtext_block_text","type":"richtext","label":"Block Text","default":"<p>Experience seamless drag-and-drop functionality for effortless design.<\/p>"}]}],"settings":[{"id":"checkbox_show_tagline","type":"checkbox","label":"Show Tagline","default":true},{"id":"text_tagline","type":"text","label":"Tagline","default":"Design"},{"id":"text_main_headline","type":"text","label":"Main Headline","default":"Unleash Your Creativity with Our Planner"},{"id":"text_headline_body","type":"text","label":"Headline Body","default":"Our 3D planning software empowers you to visualize your space like never before. With intuitive tools and starter templates, designing your dream cabinetry has never been easier."},{"id":"checkbox_show_cta_buttons","type":"checkbox","label":"Show CTA Buttons","default":true},{"id":"url_button_2_url","type":"url","label":"Button 2 URL"},{"id":"text_button_2_text","type":"text","label":"Button 2 Text","default":"Explore"},{"id":"url_button_1_url","type":"url","label":"Button 1 URL"},{"id":"text_button_1_text","type":"text","label":"Button 1 Text","default":"Explore"}],"presets":[{"name":"Layout 241","category":"Liquify","blocks":[{"type":"key-feature-block"}]}]} {% endschema %}